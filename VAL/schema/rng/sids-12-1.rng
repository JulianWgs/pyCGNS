<?xml version="1.0" encoding="UTF-8"?>
<grammar xmlns="http://relaxng.org/ns/structure/1.0">
  <define name="ReferenceState_t">
    <element name="ReferenceState_t">
      <attribute name="Name">
        <ref name="Name_t"/>
      </attribute>
      <optional>
        <attribute name="DataClass">
          <ref name="DataClass_t"/>
        </attribute>
      </optional>
      <interleave>
        <optional>
          <ref name="ReferenceStateDescription_t"/>
        </optional>
        <zeroOrMore>
          <ref name="Descriptor_t"/>
        </zeroOrMore>
        <zeroOrMore>
          <ref name="DataArray_t"/>
        </zeroOrMore>
        <optional>
          <ref name="DimensionalUnits_t"/>
        </optional>
        <zeroOrMore>
          <ref name="UserDefinedData_t"/>
        </zeroOrMore>
      </interleave>
    </element>
  </define>
  <define name="ReferenceStateDescription_t">
    <element name="ReferenceStateDescription_t">
      <attribute name="Name">
        <ref name="Name_t"/>
      </attribute>
      <ref name="DescriptorExt"/>
    </element>
  </define>
</grammar>
