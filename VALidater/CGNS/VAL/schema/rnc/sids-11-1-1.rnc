BaseIterativeData_t = element BaseIterativeData_t 
{
 attribute Name          {Name_t},
 attribute NumberOfSteps {xsd:unsignedInt},
 attribute DataClass     {DataClass_t}?,

 ( (TimeValues_t | IterationValues_t | 
   (TimeValues_t & IterationValues_t))
 & NumberOfZones_t ?
 & NumberOfFamilies_t ?
 & ZonePointers_t ?
 & FamilyPointers_t ?
 & DataArray_t *
 & Descriptor_t *
 & DimensionalUnits_t ?
 & UserDefinedData_t *
 )
}

TimeValues_t = element TimeValues_t 
{
 attribute Name            {xsd:string "TimeValues"},
 attribute DataType        {xsd:string "RealDouble"},
 attribute Dimension       {xsd:unsignedInt "1"},
 attribute DimensionValues {xsd:string},

 DataArrayExt
}

